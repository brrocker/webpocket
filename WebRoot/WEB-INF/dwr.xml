<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create javascript="Account" creator="spring">
			<param name="beanName" value="account_dwr"></param>
		</create>
		<create javascript="Text" creator="spring">
			<param name="beanName" value="text_dwr"></param>
		</create>
		
		<convert converter="bean" match="com.webpocket.data.pojo.*"/>
	</allow>
</dwr>